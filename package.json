{
  "name": "chatgpt-comment-translate",
  "displayName": "chatgpt-comment-translate",
  "description": "chatgpt translate source for comment-translate",
  "version": "1.0.0",
  "publisher": "kitiho",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "translateSource",
    "comment translate",
    "chatgpt",
    "chatgpt translate",
    "翻译",
    "注释"
  ],
  "homepage": "https://github.com/kitiho/vscode-comment-translate-chatgpt#readme",
  "bugs": {
    "url": "https://github.com/kitiho/vscode-comment-translate-chatgpt/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kitiho/vscode-comment-translate-chatgpt.git"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "icon": "image/logo.png",
  "extensionDependencies": [
    "intellsmi.comment-translate"
  ],
  "contributes": {
    "configuration": [
      {
        "title": "Chatgpt translate Configuration",
        "type": "Object",
        "properties": {
          "chatgptTranslate.useMode": {
            "order": 0,
            "enum": [
              "openAI",
              "unofficialProxy"
            ],
            "enumDescriptions": [
              "Using OpenAI Key to call the ChatGPT service.",
              "Using unofficial proxy service to call ChatGPT."
            ],
            "default": "openAI",
            "markdownDescription": "Supported modes for using ChatGPT for translation. Default is openAI."
          },
          "chatgptTranslate.authKey": {
            "order": 1,
            "type": "string",
            "markdownDescription": "You need an authentication key to access to the API.[Accessing the API](https://openai.com)"
          },
          "chatgptTranslate.accessToken": {
            "order": 2,
            "type": "string",
            "markdownDescription": "You just need to provide a ChatGPT accessToken instead of an OpenAI API key.[Support Details](https://github.com/transitive-bullshit/chatgpt-api#usage---chatgptunofficialproxyapi)"
          },
          "chatgptTranslate.reverseProxyUrl": {
            "order": 3,
            "type": "string",
            "markdownDescription": "Calling the unofficial ChatGPT service URL.[Details](https://github.com/transitive-bullshit/chatgpt-api#reverse-proxy)"
          },
          "chatgptTranslate.conversationId": {
            "order": 4,
            "type": "string",
            "markdownDescription": "If you do not provide a conversationId, a new conversationId will be generated each time you perform a translation, leading to the creation of a new conversation.To obtain an existing conversationId in OpenAI."
          },
          "chatgptTranslate.parentMessageId": {
            "order": 5,
            "type": "string",
            "markdownDescription": "If you provide a conversationId, the chatGPT API requires a parentMessageId to be present.To obtain an existing parentMessageId in OpenAI."
          }
        }
      }
    ],
    "translates": [
      {
        "translate": "chatgpt",
        "title": "ChatGPT translate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "rm -rf ./out/* && rollup --config",
    "watch": "rm -rf ./out/* && rollup -w --config",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "rollup": "^3.27.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "0.21.4",
    "chatgpt": "^5.2.5",
    "comment-translate-manager": "^0.0.2",
    "node-fetch": "3.3.1",
    "node-get-random-values": "^1.1.0",
    "querystring": "^0.2.1"
  }
}
